
terraform {
  required_providers {
    aws = {
      source = "hashicorp/aws"
    }
  }
  backend "s3" {}
}

provider "aws" {
  default_tags {
    tags = {
      AlwasyUp  = var.alwaysup
      Name      = var.user_name
      CreatedBy = var.created_by
      Contact   = var.my_email_address
      DeleteBy  = var.deleteby
    }
  }
  region = "{{ tf_backend_config.region }}"
}
